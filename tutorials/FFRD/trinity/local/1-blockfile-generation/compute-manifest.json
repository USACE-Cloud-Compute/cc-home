{
    "manifest_name": "block-generation",
    "plugin_definition": "FFRD-SEED-GENERATOR",
    "stores": [
        {
            "name": "FFRD",
            "store_type": "S3",
            "profile": "FFRD",
            "params": {
                "root": "/model-library/ffrd-trinity"
            }
        }
    ],
    "outputs": [
        {
            "name": "blocks.json",
            "paths": {
                "default": "conformance/simulations/blocks.json"
            },
            "store_name": "FFRD"
        }
    ],
    "actions": [
        {
            "name": "block-generation-fixed-length",
            "type": "block-generation-fixed-length",
            "description": "generating blocks",
            "attributes": {
                "target_total_events": 50,
                "blocks_per_realization": 5,
                "target_events_per_block": 2,
                "seed": 4321,
                "outputdataset_name": "blocks.json",
                "store_type": "json"
            }
        }
    ]
}