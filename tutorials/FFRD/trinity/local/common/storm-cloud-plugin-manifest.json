{
    "name":"STORMHUB",
    "image_and_tag":"ghcr.io/usace-cloud-compute/cc-stormhub-cloud:v0.0.1",
    "description":"StormCloud runner",
    "command":["python3.12", "main.py"],
    "compute_environment":{
        "vcpu":"8",
        "memory":"15000"
    },
    "environment":[
        {
            "name":  "CC_AWS_DEFAULT_REGION",
            "value": "us-east-1"
        },
        {
            "name":  "CC_AWS_S3_BUCKET",
            "value": "ccstore"
        },
        {
            "name":  "CC_AWS_ENDPOINT",
            "value": "http://host.docker.internal:9000"
        },
        {
            "name":  "FFRD_AWS_DEFAULT_REGION",
            "value": "us-east-1"
        },
        {
            "name":  "FFRD_AWS_S3_BUCKET",
            "value": "project-data"
        },
        {
            "name":  "FFRD_AWS_ENDPOINT",
            "value": "http://host.docker.internal:9000"
        },
        {
            "name":  "CC_ROOT",
            "value": "cc_store"
        }
    ],
    "credentials":[
        {
            "name":  "CC_AWS_ACCESS_KEY_ID",
            "value": "secretsmanager:CC_AWS_ACCESS_KEY_ID::"
        },
        {
            "name":  "CC_AWS_SECRET_ACCESS_KEY",
            "value": "secretsmanager:CC_AWS_SECRET_ACCESS_KEY::"
        },
        {
            "name":  "FFRD_AWS_ACCESS_KEY_ID",
            "value": "secretsmanager:FFRD_AWS_ACCESS_KEY_ID::"
        },
        {
            "name":  "FFRD_AWS_SECRET_ACCESS_KEY",
            "value": "secretsmanager:FFRD_AWS_SECRET_ACCESS_KEY::"
        }
    ]
}