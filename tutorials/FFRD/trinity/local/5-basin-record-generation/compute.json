{
    "name": "Basin Record Generation",
    "provider":{
        "type":"docker",
        "concurrency": "1",
        "queue": "docker-local"
    },
    "plugins": ["../common/basin-record-generation-plugin-manifest.json"],
    "event":{
        "compute-manifests":["compute-manifest.json"]
    },    
    "generator":{
        "type": "stream",
        "file": "../common/stream_list.csv",
        "delimiter": ","
    },
    "secrets-manager":{
        "type": "env",
        "secrets":{
            "secretsmanager:AWS_ACCESS_KEY_ID::":"AWS_ACCESS_KEY_ID",
            "secretsmanager:AWS_SECRET_ACCESS_KEY::":"AWS_SECRET_ACCESS_KEY"
        }
    }
}
